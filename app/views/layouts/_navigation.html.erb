<% if current_user %>
    <%= react_component 'MenuBarIndex', { logged: !current_user.nil?, user_id: current_user.id }, { prerender: true } %>
<% else %>
    <%= react_component 'MenuBarIndex', { logged: !current_user.nil?, user_id: nil }, { prerender: true } %>
<% end %>

<%= javascript_pack_tag 'common' %>
<%= javascript_pack_tag 'menu_bar' %>