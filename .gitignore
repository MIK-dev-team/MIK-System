# Ignore RubyMine things
.idea
.generators

# Ignore bundler config.
/.bundle
/vendor/bundle

# Ignore all logfiles and tempfiles.
/log
/tmp
!/log/.keep
!/tmp/.keep

# Merge file backups
*.orig

/node_modules
/yarn-error.log

.byebug_history
/public/packs

# Test and misc stuff
.rspec
/public/system
/public/assets/**

# Default database schema
/db/schema.rb

# Test coverage
/coverage

config/initializers/secret_token.rb

*.rbc
capybara-*.html
/db/*.sqlite3
/db/*.sqlite3-journal
/coverage/
/spec/tmp
*.orig
rerun.txt
pickle-email-*.html
